body {
	background: #FFF;
}
.row_pad{
/* 	    padding: 0px 40px; */
}
.grid_pad{        padding: 5px 5px 24px;}
.proxy_padding{    padding: 5px 20px 24px;}
.agent_but{padding: 2px 2px;font-size: 12px}
.agent_all{    font-size: 12px;　padding: 2px 12px !important;　border-radius: 5px !important;}




@media screen and (min-width:768px) {
    html,body{
	height: 100% !important;
    font-size: 12px;
    color: #2c3e50;
    background: #fFF;
}
}

*{
	margin:0;
	padding:0;
	list-style:none;
}

@media screen and (min-width:320px) {
	
html,body{
/* 	    height: 100%; */
    font-size: 12px;
    color: #2c3e50;
    background: #fFF;
    min-height: 600px;
    min-width: 360px;
}
}

input,select,textarea{
	outline: none;
	border: none;
}
.table{
	margin-top: 20px;
}
table.table-bordered{
	border: 1px solid #52b2d8;
}
table.table-bordered thead th,table.table-bordered tbody td{
	border: 1px solid #52b2d8;
}
/* table.table thead th,table.table tbody td{
	border-bottom: 1px solid #52b2d8;
	text-align: center;
	vertical-align: middle;
} */
.modal-title{
	font-size: 14px;
}
.control-textarea{
	padding: 4px 6px;
	background-color: #eee;
	border: 1px solid #ccc;
	border-radius: 6px;
	vertical-align: middle;
}
a:link,a:hover,a:active,a:visited{
	text-decoration:none;
	color:#fff;
}
.clear::after{
	content:'';
	display: block;
	clear:both;
	visibility: hidden;
}

.wrap{
	min-height: 100%;
}
.main{
	padding-bottom: 40px;
	overflow: auto;
}
.top-header{
	position: relative;
	height: 54px;
	background: url("../images/top_header_bg.png") 0 0 repeat-x;
}
.top-header h4{
	margin-top: 0;
	text-align: center;
	color: #fff;
	line-height: 54px;
}
.top-header .top-btn-left{
	position: absolute;
	left: 8px;
	top: 20px;
}
.top-header .top-btn-right{
	position: absolute;
	right: 8px;
	top: 20px;
}
.top-btn-left a, .top-btn-right a{
	margin-left: 10px;
}
.footer{
	height: 40px;
	margin-top: -40px;
	text-align: center;
	color: #fff;
	background-color: #499EBE;
}
.copyright{
	padding-top: 12px;
}
.alert{
	margin-bottom: 10px;
	padding: 10px;
}
.well{
	padding: 10px;
	font-size: 12px;
	margin-top: 10px;
	margin-bottom: 10px;
	line-height: 20px;
}
.well p{
	margin-bottom: 6px;
	font-size: 12px;
}
.well span{
	width: 50%;
	display: inline-block;
}
.span-label{
	font-weight: bold;
	text-align: right;
}
.span-content{
	text-align: left;
}
.radio label{
	margin-left: 4px;
	margin-right: 4px;
}
.form-inline{
	overflow: hidden;
}
.control-btn{
	color: #fff;
	background-color: #5091af;
}
.control-btn:active{
	color: #fff;
}

/* ç™»å½•*/
.login-main{
	margin-top: 10px;
	overflow: hidden;
}
.login-title{
	position: relative;
	height: 54px;
	padding: 0 15px;
	font-size: 16px;
	line-height: 54px;
	background: url("../images/login_title_bg.png") 0 0 repeat-x;
}
.login-content li{
	text-align: center;
}
.login-content>li~li{
	margin-top: 8px;
}
.login-item{
	display: inline-block;
	width: 156px;
	height: 32px;
}
.login-wx{
	background: url("../images/login_wx.png") scroll no-repeat center;
}
.login-phone{
	background: url("../images/login_phone.png") scroll no-repeat center;
}
.login-user{
	background: url("../images/login_user.png") scroll no-repeat center;
}
.login-back-btn{
	position: absolute;
	right: 15px;
	bottom: 8px;
}
a.captcha-tip{
	display: inline-block;
	height: 36px;
	margin-left: 10px;
	color: #00A0E9;
	line-height: 36px;
}
.find-passwd{
	position: absolute;
	right: 0;
	bottom: 0;
}
.find-passwd a{
	color: #555;
}
/* ä¸»é¡µ*/
.container .user-box{
	margin: 15px 0;
	padding: 6px;
	font-size: 12px;
	background-color: #F3F9FD;
	border: 1px solid #75BDEC;
}
.container .user-box img{
	width: 60px;
}
.user-box-content{
	float: left;
}
.user-box>div+div{
	padding-left: 10px;
	line-height: 20px;
}
.container .menu{
	margin-bottom: 15px;
}
.container .menu li{
/* 	height:35px; */
	margin-top: 0;
	margin-bottom: 0;
	border: 1px solid #75BDEC;
	border-top: none;
}
.container .menu:first-child{
	border-top: 1px solid #75BDEC;
}
.menu li a{
	position: relative;
	font-size: 14px;
	font-weight: bold;
	color: #2786AB;
	background-color: #F3F9FD;
	border-radius: 0px;
}
.menu li a:hover{
	color: #fff;
	background-color: #78BDD8;
}
.menu li a:active{
	color: #fff;
	background-color: #78BDD8;
}
.show-page{
	position: absolute;
	top: 6px;
	right: 10px;
}
.tip-box{
	padding-top: 6px;
	border-top: 1px solid #3CABDE;
}
.tip-box p{
	margin-top: 6px;
	margin-bottom: 6px;
}
.diabled-resize{
	resize: none;
}
.user-modal .row{
	margin-left: auto;
	margin-right: auto;
	margin-bottom: 8px;
}
.user-modal .col-xs-3{
	text-align: right;
	padding-right: 0px;
	padding-left: 8px;
}
.user-modal .row-data{
	margin-bottom: 8px;
}
.user-data{
	padding: 8px 10px;
	border: 1px solid #ddd;
	border-radius: 6px;
	background: #fefefe;
}
.user-data .row{
	padding-bottom: 10px;
}
.user-data .col-xs-3{
	text-align: right;
	padding-right: 0px;
	padding-left: 8px;
}
.publish-img{
	display: inline-block;
	width: 100%;
}


/* åŠŸèƒ½é¡µé¢*/
.game-title{
	text-align: center;
}
.query-box{
	font-size: 12px;
	text-align: left;
	overflow: hidden;
}
.search-box{
	display: inline-block;
}
.search-box input{
	height: auto;
	width: 80px;
	font-size: 12px;
}
.search-box button{
	padding: 6px 8px;
	font-size: 12px;
}
@media screen and (min-width:768px) {
	.query-box{ text-align: center;}
}
.info-tip{
	text-align: center;
	margin: 20px 0;
}
.control-row{
	margin-bottom: 8px;
}
@media screen and (min-width:768px) {
	.control-row{ display: inline-block;}
}
.common-label{
	padding: 4px 8px;
	line-height: 34px;
}
.query-btn{
	width: 100%;
}
@media screen and (min-width:768px) {
	.query-btn{ width: auto;margin-left: 10px;}
}
.form-input{
	height: 34px;
	padding: 6px 12px;
	font-size: 14px;
	color: #555;
	background-color: #fcfcfc;
	border: 1px solid #ccc;
	border-radius: 4px;
}
.form-select{
	height: 34px;
	padding: 6px 12px;
	font-size: 14px;
	color: #555;
	background-color: #fcfcfc;
	border: 1px solid #ccc;
	border-radius: 4px;
}
.table-btn{
	font-size: 12px;
	padding: 4px 6px;
}
.float-qrcode{
	position: fixed;
	right: 6px;
	top: 100px;
	width: 74px;
	padding-top: 4px;
	text-align: center;
	background-color: #fefefe;
	border: 1px solid #ccc;
	-webkit-border-radius: 6px;
	-moz-border-radius: 6px;
	border-radius: 6px;
	-webkit-box-shadow: 2px 2px 2px hsla(0, 0%, 26.667%, 0.8);
	-moz-box-shadow: 2px 2px 2px hsla(0, 0%, 26.667%, 0.8);
	box-shadow: 2px 2px 2px hsla(0, 0%, 26.667%, 0.8);
}
.float-qrcode img{
	width: 60px;
	margin-left: auto;
	margin-right: auto;
}
.float-qrcode span{
	display: inline-block;
	margin-top: 2px;
	color: #888;
}
.content-null{
	color: #888;
}
.page-title{
	position: relative;
	margin-top: 10px;
	margin-bottom: 10px;
	border-bottom: 1px solid #3CABDE;
}
.page-title h4{
	font-weight: 700;
}
.back-btn{
	position: absolute;
	right: 0;
	bottom: 2px;
}
.page-str{
	text-align: center;
}
.pagination{
	margin: 6px 0;
}
@media screen and (max-width: 360px) {
	@media screen and (orientation: portrait) {
		.copyright{
			padding-top: 6px;
		}
	}
}
