<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qs.webside.member.mapper.BusinessMapper">

  <!--<sql id="Base_Column_List">
    id, apkid, name, version, size, date, status, filepath
  </sql>-->

  <select id="queryListMapByPage" parameterType="map" resultType="java.util.HashMap">
    SELECT a.`name`,a.phone,b.cname
    FROM
    sc_majiang.memberbusiness AS a
    LEFT JOIN sc_majiang.app_company AS b
    ON a.company=b.cid ORDER BY mktime
  </select>

</mapper>