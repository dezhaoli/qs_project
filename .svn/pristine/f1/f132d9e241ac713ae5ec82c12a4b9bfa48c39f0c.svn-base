  <!-- 根据活动类型查询，该活动类型对应的奖品兑换记录总个数 -->
  <select id="checkAwardRecordSumByActiType" parameterType="integer" resultType="integer">
    SELECT SUM(a.award_num) AS expNum
    FROM acti_award_record a ,acti_center ac
    WHERE ac.type = a.type
    AND a.create_time &gt;= ac.start_time
    AND a.create_time &lt;= ac.end_time
