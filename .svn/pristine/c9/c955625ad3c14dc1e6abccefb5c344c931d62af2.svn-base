<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qs.log.game.mapper.ClubMemberMapper" >
  <resultMap id="BaseResultMap" type="com.qs.log.game.model.ClubMember" >
    <result column="mid" property="mid" jdbcType="INTEGER" />
    <result column="clubid" property="clubid" jdbcType="INTEGER" />
    <result column="mtime" property="mtime" jdbcType="INTEGER" />
  </resultMap>
  
   <sql id="Base_Column_List" >
    mid,clubid,mtime 
  </sql>
  <insert id="insert" parameterType="com.qs.log.game.model.ClubMember" >
    insert into club_member (mid, clubid, mtime
      )
    values (#{mid,jdbcType=INTEGER}, #{clubid,jdbcType=INTEGER}, #{mtime,jdbcType=INTEGER}
      )
  </insert>
  <insert id="insertSelective" parameterType="com.qs.log.game.model.ClubMember" >
    insert into club_member
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="mid != null" >
        mid,
      </if>
      <if test="clubid != null" >
        clubid,
      </if>
      <if test="mtime != null" >
        mtime,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="mid != null" >
        #{mid,jdbcType=INTEGER},
      </if>
      <if test="clubid != null" >
        #{clubid,jdbcType=INTEGER},
      </if>
      <if test="mtime != null" >
        #{mtime,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
  
  <select id="getClubMemberInfo" resultType="map" parameterType="map">
   SELECT m.mid,mf.name,
	CASE WHEN (SELECT count(mid) FROM club_mids ms where ms.clubid = #{cmid } and ms.mid=m.mid)>0 THEN 0 ELSE 1 END AS openroom
	 FROM 
	 club_member m ,${dbName} mf 
	where m.mid=mf.mid and m.clubid=#{cmid }
 	<if test="mid !=null and  mid !='' ">
	 and m.mid=#{mid }
	 </if>
  </select>
  
  <select id="selectClubMember" parameterType="com.qs.log.game.model.ClubMember" resultMap="BaseResultMap">
   	select 
    <include refid="Base_Column_List" />
  		from club_member 
  	<where>
	 	clubid=#{clubid } and mid= #{mid}
	</where>
  </select>
  
  
  <select id="selectCountClubMember" parameterType="java.lang.Integer" resultType="java.lang.Integer">
  SELECT COUNT(mid) from club_member 
  where clubid=#{cmid} 
  </select>
  
  <delete id="deleteByPrimaryKey" parameterType="com.qs.log.game.model.ClubMember" >
    delete from club_member
    where clubid = #{clubid}  and mid = #{mid} 
  </delete>
  
  <select id="getByPrimaryKeyList" parameterType="java.lang.Integer" resultMap="BaseResultMap">
	  SELECT 
	  <include refid="Base_Column_List" />
	   from club_member 
	  where mid=#{mid}  
  </select>
</mapper>