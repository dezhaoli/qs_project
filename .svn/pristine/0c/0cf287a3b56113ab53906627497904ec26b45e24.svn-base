            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!-- java mapper xml生成路径 -->
        <sqlMapGenerator targetPackage="member"
                         targetProject="E:\idea_java_project\second_project\qs-mapper\src\main\resources\mybatis\mapper\webside">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <!-- mapper java -->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.qs.webside.member.mapper"
                             targetProject="E:\idea_java_project\second_project\qs-mapper\src\main\java">
            <property name="enableSubPackages" value="true"/>
            <property name="rootInterface" value="com.qs.common.base.basemapper.IBaseMapper"/>
        </javaClientGenerator>

