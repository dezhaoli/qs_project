        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- java 模型对象生成路径 -->
        <javaModelGenerator targetPackage="com.qs.webside.member.model"
