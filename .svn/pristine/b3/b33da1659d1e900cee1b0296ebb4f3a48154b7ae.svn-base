<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qs.mainku.game.mapper.AgentMidsMapper" >
  <resultMap id="BaseResultMap" type="com.qs.mainku.game.model.AgentMids" >
    <result column="amid" property="amid" jdbcType="INTEGER" />
    <result column="mid" property="mid" jdbcType="INTEGER" />
  </resultMap>
  <insert id="insert" parameterType="com.qs.mainku.game.model.AgentMids" >
    insert into agent_mids (amid, mid)
    values (#{amid,jdbcType=INTEGER}, #{mid,jdbcType=INTEGER})
  </insert>
  <insert id="insertSelective" parameterType="com.qs.mainku.game.model.AgentMids" >
    insert into agent_mids
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="amid != null" >
        amid,
      </if>
      <if test="mid != null" >
        mid,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="amid != null" >
        #{amid,jdbcType=INTEGER},
      </if>
      <if test="mid != null" >
        #{mid,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
  
  <select id="getAgentGrantByMid" resultMap="BaseResultMap" parameterType="java.lang.Integer" >
    select 
    amid, mid 
    from agent_mids
    where  mid = #{mid,jdbcType=INTEGER}
     limit 0,1
  </select>
  
  
</mapper>