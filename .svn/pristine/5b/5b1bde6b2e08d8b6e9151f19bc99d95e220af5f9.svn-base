<div  style=" margin-left: 100px;overflow:hidden;">
	 <div class="row qs_phb_body" >
	  <div style="background-color:#40373A;margin: 90px 45px 0px;color: #FFF;    font-size: 18px; ">
		   <table class="table table-striped qs_ph_table text-center" style="margin-bottom: 5px; ">
			    <tr style="background-color: #C13329;width: 104px">
			      <td style="width: 110px !important;"></td>
			      <td style="width: 215px !important;">头像</td>
			      <td style="width: 215px !important;">昵称</td>
			      <td style="width: 215px !important;">积分</td>
			    </tr>
			</table>
			<div style="overflow-y: scroll;height: 415px;">
			
		    <table class="table table-striped qs_ph_table text-center"  >
			   <!--  <tr style="background-color: #C13329">
			      <td></td>
			      <td>头像</td>
			      <td>昵称</td>
			      <td>积分</td>
			    </tr> -->
			    <tr ng-repeat="item in data">
			      <td style="width: 104px !important;    height: 100px;" ng-if="$index ==0"><img alt="" src="img/ranking/1.png" ></td>
			      <td style="width: 104px !important;    height: 100px;" ng-if="$index ==1"><img alt="" src="img/ranking/2.png" ></td>
			      <td style="width: 104px !important;    height: 100px;" ng-if="$index ==2"><img alt="" src="img/ranking/3.png" ></td>
			      <td style="width: 104px !important;    height: 100px;" ng-if="$index >2">{{$index+1}}</td>
			      <td style="width: 215px !important;    height: 100px;"><img alt="" class="qs_ph_imghead" src="{{item.icon}}" ></td>
			      <td style="width: 215px !important;    height: 100px;">{{item.name}}</td>
			      <td style="width: 215px !important;    height: 100px;">{{item.integral}}</td>
			    </tr>
			   
			</table>
			</div>
		</div>
	</div>
	<!-- <div class="row qs_phb_foot" >
	    
	</div> -->
</div>

