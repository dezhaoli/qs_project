<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE generatorConfiguration  
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">  
  
<generatorConfiguration>  
    <classPathEntry location="D:\java\mysql\mysql-connector-java-5.1.39.jar" />
      <context id="DB2Tables" targetRuntime="MyBatis3">
		<commentGenerator>
            <property name="suppressDate" value="true" />
            <property name="suppressAllComments" value="true" />
        </commentGenerator>  
  
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"  
            connectionURL="jdbc:mysql://127.0.0.1:3306/psyllidae_cfg"
            userId="root" password="123456">
        </jdbcConnection>
 
          <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />  
        </javaTypeResolver>
		
		<!-- java 模型对象生成路径 -->
        <javaModelGenerator targetPackage="com.qs.cfg.acti.model"  
            targetProject="D:\java\myHome\qs-model\src\main\java">   
            <property name="enableSubPackages" value="true" />  
            <property name="trimStrings" value="true" />  
        </javaModelGenerator>  
  		
  		<!-- java mapper xml生成路径 -->
        <sqlMapGenerator targetPackage="acti"  
            targetProject="D:\java\myHome\qs-mapper\src\main\resources\mybatis\mapper\cfg">  
            <property name="enableSubPackages" value="true" />  
        </sqlMapGenerator>  
      
        <!-- mapper java -->
		<javaClientGenerator type="XMLMAPPER"  
            targetPackage="com.qs.cfg.acti.mapper" targetProject="D:\java\myHome\qs-mapper\src\main\java">  
            <property name="enableSubPackages" value="true" /> 
            <property name="rootInterface" value="com.qs.common.base.basemapper.IBaseMapper"/>
		</javaClientGenerator>
		
		 <table schema="psyllidae_cfg" tableName="acti_systemstorecfg" domainObjectName="Store" enableCountByExample="false"
			enableUpdateByExample="false"
			enableDeleteByExample="false"
			enableSelectByExample="false"
			selectByExampleQueryId="false">
		</table>

					
	</context>  
</generatorConfiguration> 